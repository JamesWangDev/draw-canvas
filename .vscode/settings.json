{
  "files.exclude": {
    "**/logs": true,
    "**/*.log": true,
    "**/npm-debug.log*": true,
    "**/yarn-debug.log*": true,
    "**/yarn-error.log*": true,
    "**/pids": true,
    "**/*.pid": true,
    "**/*.seed": true,
    "**/*.pid.lock": true,
    "**/.dummy": true,
    "**/lib-cov": true,
    "**/coverage": true,
    "**/.nyc_output": true,
    "**/.grunt": true,
    "**/.snapshots/": true,
    "**/bower_components": true,
    "**/.lock-wscript": true,
    "build/Release": true,
    "**/node_modules/": true,
    "**/jspm_packages/": true,
    "**/typings/": true,
    "**/.npm": true,
    "**/.eslintcache": true,
    "**/.node_repl_history": true,
    "**/*.tgz": true,
    "**/.yarn-integrity": true,
    "**/.next/": false,
    "**/dist/": true,
    "**/build/": true,
    "**/.now/": true,
    "**/.vercel/": true,
    "**/.google.json": true,
    "**/public/location": true
  },
  "editor.formatOnSave": true,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120
}
